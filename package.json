  },
  "peerDependencies": {
    "discord.js": "^12.3.1"
  },
  "devDependencies": {
    "@types/node": "^13.13.25",
    "clean-jsdoc-theme": "^2.2.11",
    "docgen": "github:dirigeants/docsgen",
    "eslint": "^6.8.0",
    "eslint-config-klasa": "github:dirigeants/klasa-lint",
    "eslint-plugin-markdown": "^1.0.2",
    "markdownlint-cli": "^0.23.2",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=10.1.0"
  },
